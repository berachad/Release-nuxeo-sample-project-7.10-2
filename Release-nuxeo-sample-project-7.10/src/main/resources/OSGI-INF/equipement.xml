<extension target="org.nuxeo.ecm.platform.types.TypeService" point="types">
    <type id="folderequipement">
      <label>label.dossier.equipement</label> 
      <icon>/img/dossier_equipement.png</icon>
      <bigIcon>/img/dossier_equipement.png</bigIcon>
      <description>Dossier Equipement</description>
      <default-view>view_documents</default-view>
      <subtypes>
        <type>Document_equipe</type>
      </subtypes>
      <layouts mode="create">
        <layout>layout@folderequipement-create</layout>
         <layout>heading_classP</layout>
      </layouts>
      <layouts mode="edit">
