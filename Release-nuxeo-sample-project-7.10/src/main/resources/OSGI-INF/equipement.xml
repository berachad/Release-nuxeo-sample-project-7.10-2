        <contentView showInExportView="false">Document_equipe</contentView>
      </contentViews>
      <contentViews category="trash_content">
        <contentView showInExportView="false">document_trash_content</contentView>
      </contentViews>
    </type>
    <type id="Workspace">
      <subtypes>
        <type>folderequipement</type>
      </subtypes>
    </type>
    <type id="Folder">
      <subtypes>
        <type>folderequipement</type>
      </subtypes>
    </type>
    <type id="Document_equipe" remove="true" />
