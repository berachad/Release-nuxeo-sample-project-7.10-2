        <layout>layout@Document_equipe-create</layout>
        <layout>file</layout>
      </layouts>
      <layouts mode="edit">
        <layout>layout@Document_equipe-edit</layout>
        <layout>file</layout>
      </layouts>
      <layouts mode="view">
        <layout>layout@Document_equipe-view</layout>
         <layout>heading_classP</layout>
      </layouts>
    </type>
    <type id="Folder">
      <subtypes>
        <type>Document_equipe</type>
      </subtypes>
    </type>
    
